		<div class="small-12 columns">
			<div class="alert-box success radius">
				<ul>
				<% _.each(flash.msg.success, function (msg) { %>
					<li><%= msg %></li>
				<% }) %>
				</ul>
			</div>
		</div>
	</div>
<% } %>